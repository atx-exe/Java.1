<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="L8.Exam.Controller">
    <GridPane fx:id="gpMap">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Button fx:id="bt00" onAction="#bt00" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="0"
                GridPane.rowIndex="0"/>
        <Button fx:id="bt01" onAction="#bt01" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="1"
                GridPane.rowIndex="0"/>
        <Button fx:id="bt02" onAction="#bt02" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="2"
                GridPane.rowIndex="0"/>
        <Button fx:id="bt10" onAction="#bt10" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="0"
                GridPane.rowIndex="1"/>
        <Button fx:id="bt11" onAction="#bt11" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="1"
                GridPane.rowIndex="1"/>
        <Button fx:id="bt12" onAction="#bt12" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="2"
                GridPane.rowIndex="1"/>
        <Button fx:id="bt20" onAction="#bt20" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="0"
                GridPane.rowIndex="2"/>
        <Button fx:id="bt21" onAction="#bt21" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="1"
                GridPane.rowIndex="2"/>
        <Button fx:id="bt22" onAction="#bt22" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="2"
                GridPane.rowIndex="2"/>
    </GridPane>
    <StackPane prefHeight="40.0" prefWidth="200.0">
        <Label fx:id="jlbMenu" text="Set game mod"/>
    </StackPane>
    <StackPane prefHeight="40.0" prefWidth="200.0">
        <HBox alignment="CENTER">
            <Button onAction="#onActPvP" text="PvP"/>
            <Button onAction="#onActPvPC" text="PvPC"/>
            <Button onAction="#onActPCvPC" text="PCvPC"/>
        </HBox>
    </StackPane>
</VBox>
